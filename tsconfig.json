{
    "compilerOptions": {
        "jsx": "react",
        "module": "es2015",
        "moduleResolution": "node",
        "sourceMap": true,
        "target": "es5",
        "lib": [
            "dom",
            "es5",
            "es2015",
            "scripthost"
        ],
        "typeRoots": [
            "./node_modules/@types"
        ],
        "types": [
            "node"
        ],
        "noUnusedLocals": true,
        "allowSyntheticDefaultImports": true
    }
}
